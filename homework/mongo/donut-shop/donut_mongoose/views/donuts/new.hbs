<main>
	<h2>Create Your Own Donut!</h2>
	<a href="/">Return to Home Page</a>
	<div id="edit-form" class="donut-container-show">
		<main>
			<h2>Delicious donuts! Handmade fresh daily!</h2>
			{{#each donuts}}
			<div class="donut-container">
				<img src="{{this.img}}" class="main-img">
				<h2>
					<a href="/{{this._id}}">{{this.name}}</a>
				</h2>
				<p>${{this.price}} each</p>
			</div>
			{{/each}}
		</main>
	</div>
</main>